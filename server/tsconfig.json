{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "rootDir": "./src",
    "outDir": "./dist",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}